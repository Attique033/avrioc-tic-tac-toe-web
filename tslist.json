{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "no-console": [
      true,
      "log",
      "error"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-access": [
      true,
      "no-public"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ]
  },
  "rulesDirectory": []
}
